@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix idpy: <http://harrisonconsoles.com/lv2/inlinedisplay#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit:  <http://lv2plug.in/ns/extensions/units#> .

<http://kxstudio.sf.net/carla/plugins/carlapatchbay>
    a lv2:Plugin, doap:Project ;

    lv2:optionalFeature <http://lv2plug.in/ns/ext/state#threadSafeRestore> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
                        <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:extensionData <http://lv2plug.in/ns/ext/options#interface> ;
    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;
    lv2:extensionData <http://lv2plug.in/ns/ext/worker#interface> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    ui:ui <http://kxstudio.sf.net/carla/ui-ext> ;

    lv2:port [
        a lv2:InputPort, atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ,
                      <http://lv2plug.in/ns/ext/time#Position> ;
        lv2:designation lv2:control ;
        lv2:index 0 ;
        lv2:symbol "lv2_events_in" ;
        lv2:name "Events Input" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
        lv2:index 1 ;
        lv2:symbol "lv2_events_out" ;
        lv2:name "Events Output" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_freewheel" ;
        lv2:name "Freewheel" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:designation <http://lv2plug.in/ns/lv2core#freeWheeling> ;
        lv2:portProperty lv2:toggled, <http://lv2plug.in/ns/ext/port-props#notOnGUI> ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] , [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 4 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 5 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] , [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 6 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    doap:developer [ foaf:name "falkTX" ] ;
    doap:license <http://opensource.org/licenses/GPL-2.0> ;
    doap:name "Carla-Patchbay" ;

    lv2:microVersion 25 ;
    lv2:minorVersion 10 ;
    lv2:symbol "carlapatchbay" .
