    doap:homepage <https://kx.studio/carla> ;
    doap:maintainer [
        foaf:homepage <https://falktx.com/> ;
        foaf:mbox <mailto:falktx@falktx.com> ;
        foaf:name "Filipe Coelho" ;
    ] ;
    doap:name "Carla" ;
    doap:shortdesc "fully-featured audio plugin host." ;
    lv2:microVersion 25 ;
    lv2:minorVersion 10 ;
    lv2:symbol "carla" .

