@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix idpy: <http://harrisonconsoles.com/lv2/inlinedisplay#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit:  <http://lv2plug.in/ns/extensions/units#> .

<http://kxstudio.sf.net/carla/plugins/lfo>
    a lv2:UtilityPlugin, lv2:Plugin, doap:Project ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
                        <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:extensionData <http://lv2plug.in/ns/ext/options#interface> ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_freewheel" ;
        lv2:name "Freewheel" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:designation <http://lv2plug.in/ns/lv2core#freeWheeling> ;
        lv2:portProperty lv2:toggled, <http://lv2plug.in/ns/ext/port-props#notOnGUI> ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "mode" ;
        lv2:name "Mode" ;
        lv2:default 1.000000 ;
        lv2:minimum 1.000000 ;
        lv2:maximum 5.000000 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Triangle" ;
                         rdf:value  1.000000 ] ,
                       [ rdfs:label "Sawtooth" ;
                         rdf:value  2.000000 ] ,
                       [ rdfs:label "Sawtooth (inverted)" ;
                         rdf:value  3.000000 ] ,
                       [ rdfs:label "Sine (TODO)" ;
                         rdf:value  4.000000 ] ,
                       [ rdfs:label "Square" ;
                         rdf:value  5.000000 ] ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "speed" ;
        lv2:name "Speed" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.010000 ;
        lv2:maximum 2048.000000 ;
        unit:unit [
            a unit:Unit ;
            rdfs:label  "(coef)" ;
            unit:symbol "(coef)" ;
            unit:render "%f (coef)" ;
        ] ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "multiplier" ;
        lv2:name "Multiplier" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.010000 ;
        lv2:maximum 2.000000 ;
        unit:unit [
            a unit:Unit ;
            rdfs:label  "(coef)" ;
            unit:symbol "(coef)" ;
            unit:render "%f (coef)" ;
        ] ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "start_value" ;
        lv2:name "Start value" ;
        lv2:default 0.000000 ;
        lv2:minimum -1.000000 ;
        lv2:maximum 1.000000 ;
    ] , [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "lfo_out" ;
        lv2:name "LFO Out" ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
    ] ;

    doap:developer [ foaf:name "falkTX" ] ;
    doap:license <http://opensource.org/licenses/GPL-2.0> ;
    doap:name "LFO" ;

    lv2:microVersion 25 ;
    lv2:minorVersion 10 ;
    lv2:symbol "lfo" .
